<?xml version="1.0" encoding="UTF-8"?>
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>DGIWG Web Map Tile Service (DGIWG WMTS) - 2021</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/nsg/wmts</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-${ets-code}-${version}</local-name>
        <title>DGIWG WMTS 1.0 Conformance Test Suite</title>
        <description>Checks DGIWG WMTS 1.0 implementations for conformance to DGIWG WMTS 1.0 (DGIWG-124).</description>
        <BasicConformanceClasses>
          <conformanceClass>Preconditions</conformanceClass>
			<conformanceClass>DGIWG WMTS requirement 2</conformanceClass>
        </BasicConformanceClasses>
      </suite>
      <revision>
        <name>${project.version}</name>
        <status>Beta</status>
        <sources>
          <source>${ets-code}/${spec-version}/ctl</source>
        </sources>
        <webdir>${ets-code}/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
